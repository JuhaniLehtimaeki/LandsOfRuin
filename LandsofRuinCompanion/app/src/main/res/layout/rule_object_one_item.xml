<?xml version="1.0" encoding="utf-8"?>
<com.landsofruin.companion.view.CornerCuttingFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/character_types"
    android:layout_width="600dp"
    android:layout_height="100dp"
    android:layout_margin="8dp"
    android:background="@drawable/solid_yellow_button_background"
    app:corner1="16dp"
    app:corner2="16dp"
    app:corner3="16dp"
    app:corner4="16dp">

    <com.landsofruin.companion.view.MechaFontTextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textAllCaps="true"
        android:textColor="#FFFFFFFF"
        android:textSize="28sp" />


</com.landsofruin.companion.view.CornerCuttingFrameLayout>
